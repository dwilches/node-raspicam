{
  "name": "raspicam",
  "description": "A Node.js-based controller module for the Raspberry Pi camera. Forked from troyth/node-raspicam, rewritten in TypeScript.",
  "version": "0.0.1",
  "homepage": "https://github.com/alexeden/node-raspicam",
  "main": "dist/raspicam",
  "author": {
    "name": "Alex Eden",
    "email": "alexandereden91@gmail.com"
  },
  "scripts": {
    "build": "tsc -d -p lib",
    "build:watch": "nodemon --exec \"npm run build\" -w lib",
    "build-sync:watch": "nodemon --exec \"npm run build && npm run sync\" -w lib -w examples",
    "exec": "ts-node --project $PWD/examples examples/photo.ts",
    "exec:watch": "nodemon --exec \"npm run exec\" -w examples -w dist",
    "sync": "node rsync.js",
    "sync:watch": "nodemon --exec \"npm run sync\" -w examples -w lib",
    "postinstall": "npm run build"
  },
  "keywords": [
    "raspberry pi",
    "camera",
    "raspicam",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/alexeden/node-raspicam.git"
  },
  "bugs": {
    "url": "https://github.com/alexeden/node-raspicam/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/troyth/node-raspicam/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=6.11.3"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/lodash": "^4.14.76",
    "@types/node": "^8.0.31",
    "grunt": "~0.4.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-jshint": "~0.6.0",
    "grunt-contrib-nodeunit": "~0.2.0",
    "grunt-jsbeautifier": "~0.2.2",
    "rsync": "^0.6.1",
    "sinon": "~1.7.3",
    "typescript": "^2.5.3"
  }
}
